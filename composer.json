{
	"name": "clipstack/clipstack",
	"type": "project",
	"description": "A PHP website framework for small and medium websites.",
	"homepage": "https://clipstack.net",
	"readme": "https://github.com/JacobJoergensen/ClipStack/blob/main/README.md",
	"license": "MIT",
	"authors": [
		{
			"name": "Jacob Joergensen",
			"email": "jj@minehubsstudios.com",
			"homepage": "https://jacobjoergensen.net",
			"role": "Lead Developer"
		}
	],
	"support": {
		"issues": "https://github.com/JacobJoergensen/ClipStack/issues",
		"source": "https://github.com/JacobJoergensen/ClipStack"
	},
	"require": {
		"php": ">=8.3",
		"ext-pdo": "*",
		"ext-ctype": "*",
		"ext-mbstring": "*",
		"ext-openssl": "*"
	},
	"keywords": [
		"framework",
		"php",
		"website"
	],
	"require-dev": {
		"phpstan/phpstan": "^1.11.0",
		"phpunit/phpunit": "^11.1"
	},
	"autoload": {
		"psr-4": {
			"ClipStack\\Component\\": "src/components/",
			"ClipStack\\Component\\Backbone\\": "src/components/backbone/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/",
			"Tests\\Component\\": "tests/components/",
			"Tests\\Backbone\\": "tests/backbone/"
		}
	}
}
